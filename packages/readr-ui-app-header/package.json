{
  "name": "@readr-ui/app-header",
  "version": "1.3.0",
  "description": "",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/readr-ui-app-header.ssr.js",
  "module": "dist/readr-ui-app-header.esm.js",
  "unpkg": "dist/readr-ui-app-header.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/*.js"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@readr-ui/icon": "^1.0.4",
    "@readr-ui/icon-share": "^1.0.1",
    "@readr-ui/logo": "^2.0.0",
    "@readr-ui/radio-item": "^1.0.0",
    "@readr-ui/side-badge": "^1.1.0",
    "dayjs": "^1.8.15",
    "lodash": "^4.17.15",
    "vue-infinite-loading": "^2.4.4",
    "vue-no-ssr": "^1.1.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
